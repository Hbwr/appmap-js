@startuml
!includeurl https://raw.githubusercontent.com/getappmap/plantuml-theme/main/appmap-theme.puml
participant controllers as "controllers"
participant models as "models"
participant users as "users"
participant posts as "posts"
participant database as "database"
  [->controllers: list <size:8><:1F551:></size><color:gray> 706μs</color>
  activate controllers
    controllers->models: <u>list</u> <size:8><:1F551:></size><color:gray> 190μs</color>
    activate models
      models-[#green]>database: <color:green><u>query</u></color> $timing
      models-[#green]>models: <color:green><u>find_for_users</u></color> <size:8><:1F551:></size><color:gray> 24μs</color>
      activate models
        models->database: <u>query</u>
      models<[#green]--models: <color:green>object</color>
      deactivate models
    controllers<--models: array
    deactivate models
    controllers->users: list <size:8><:1F551:></size><color:gray> 313μs</color>
    activate users
      Loop 5 times <size:8><:1F551:></size><color:gray> 8μs</color>
        users->models: posts <size:8><:1F551:></size><color:gray> 1μs</color>
        activate models
          models-[#red]>models: <color:red><u>find_for_user</u></color> <size:8><:1F551:></size><color:gray> 30μs</color>
          activate models
            models-[#red]>database: <color:red><u>query</u></color> <size:8><:1F551:></size><color:gray> 1μs</color>
          models<[#red]--models: <color:red>array</color>
          deactivate models
        users<--models: array
        deactivate models
        Loop ~2 times <size:8><:1F551:></size><color:gray> 1μs</color>
          users->posts: show <size:8><:1F551:></size><color:gray> 1μs</color>
          activate posts
          users<--posts: string
          deactivate posts
        End
      End
    controllers<--users: array
    deactivate users
  [<--controllers: array
  deactivate controllers
@enduml